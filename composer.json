{
  "name": "codeinc/middleware-test-kit",
  "version": "1.0.1",
  "description": "A test kit for PHPUnit and PSR-15 middleware",
  "homepage": "https://github.com/CodeIncHQ/MiddlewareTestKit",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "psr/http-message": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "psr/http-server-handler": "^1.0",
    "codeinc/psr7-responses": "^2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  },
  "autoload": {
    "psr-4": {"CodeInc\\MiddlewareTestKit\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": { "CodeInc\\MiddlewareTestKit\\Tests\\": "tests/" }
  },
  "authors": [
    {
      "name": "Joan Fabr√©gat",
      "email": "joan@codeinc.fr",
      "homepage": "https://www.codeinc.fr",
      "role": "developer"
    }
  ],
  "config": {
    "preferred-install": {
      "*": "dist"
    }
  }
}
